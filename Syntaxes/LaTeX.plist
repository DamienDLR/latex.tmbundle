<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>backgroundColor</key>
	<string>#ffffff</string>
	<key>fileTypes</key>
	<array>
		<string>tex</string>
	</array>
	<key>foldingStartMarker</key>
	<string>\\begin\{.*\}</string>
	<key>foldingStopMarker</key>
	<string>\\end\{.*\}</string>
	<key>foregroundColor</key>
	<string>#333333</string>
	<key>highlightPairs</key>
	<array>
		<array>
			<string>(</string>
			<string>)</string>
		</array>
		<array>
			<string>[</string>
			<string>]</string>
		</array>
		<array>
			<string>{</string>
			<string>}</string>
		</array>
		<array>
			<string>"</string>
			<string>"</string>
		</array>
	</array>
	<key>name</key>
	<string>Latex</string>
	<key>patterns</key>
	<array>
		<dict>
			<key>begin</key>
			<string>\$\$</string>
			<key>end</key>
			<string>\$\$</string>
			<key>foregroundColor</key>
			<string>#00AA00</string>
			<key>name</key>
			<string>Non-numbered equation</string>
		</dict>
		<dict>
			<key>foregroundColor</key>
			<string>#07209D</string>
			<key>match</key>
			<string>[\[\]\{\}</string>
			<key>name</key>
			<string>Brackets</string>
		</dict>
		<dict>
			<key>begin</key>
			<string>\\(((sub)*section)|(chapter)|(paragraph)|(part))(\*?)\{</string>
			<key>end</key>
			<string>\}</string>
			<key>fontStyle</key>
			<array>
				<string>bold</string>
			</array>
			<key>foregroundColor</key>
			<string>#0000FF</string>
			<key>name</key>
			<string>Sections</string>
		</dict>
		<dict>
			<key>begin</key>
			<string>(^|[^\\])\$</string>
			<key>end</key>
			<string>\$</string>
			<key>foregroundColor</key>
			<string>#005C00</string>
			<key>name</key>
			<string>Inline equation</string>
		</dict>
		<dict>
			<key>begin</key>
			<string>\\(\(|\[)</string>
			<key>end</key>
			<string>\\(\)|\])</string>
			<key>foregroundColor</key>
			<string>#006600</string>
			<key>name</key>
			<string>Inline alt equation</string>
		</dict>
		<dict>
			<key>fontStyle</key>
			<array>
				<string>italic</string>
			</array>
			<key>foregroundColor</key>
			<string>#5C5C5C</string>
			<key>match</key>
			<string>(^|[^\\])%.*$</string>
			<key>name</key>
			<string>Comments</string>
		</dict>
		<dict>
			<key>begin</key>
			<string>\\cite[tp]*\{</string>
			<key>end</key>
			<string>\}</string>
			<key>fontStyle</key>
			<array>
				<string>underline</string>
			</array>
			<key>foregroundColor</key>
			<string>#6600CC</string>
			<key>name</key>
			<string>Citations</string>
		</dict>
		<dict>
			<key>foregroundColor</key>
			<string>#681C79</string>
			<key>match</key>
			<string>\\\w*</string>
			<key>name</key>
			<string>Tags</string>
		</dict>
	</array>
	<key>smartTypingPairs</key>
	<array>
		<array>
			<string>(</string>
			<string>)</string>
		</array>
		<array>
			<string>[</string>
			<string>]</string>
		</array>
		<array>
			<string>{</string>
			<string>}</string>
		</array>
		<array>
			<string>"</string>
			<string>"</string>
		</array>
	</array>
</dict>
</plist>
